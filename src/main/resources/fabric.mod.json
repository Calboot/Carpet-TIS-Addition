{
  "schemaVersion": 1,
  "id": "carpet-tis-addition",
  "version": "${version}",

  "name": "Carpet TIS Addition",
  "description": "More useful tools and interesting features",
  "authors": [
    "Fallen_Breath ..."
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/carpet-tis-addition",
    "issues": "https://github.com/TISUnion/Carpet-TIS-Addition/issues",
    "sources": "https://github.com/TISUnion/Carpet-TIS-Addition"
  },

  "icon": "assets/carpettisaddition/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "carpettisaddition.CarpetTISAdditionMod"
    ]
  },
  "mixins": [
    "carpet-tis-addition.mixins.json"
  ],

  "accessWidener": "carpet-tis-addition.accesswidener",
  "depends": {
    "fabricloader": ">=0.11.7",
    "carpet": "${carpet_dependency}",
    "minecraft": "${minecraft_dependency}"
  },
  "custom": {
    "modmenu:parent": "carpet"
  }
}
